{
  "expo": {
    "name": "SRM Pay",
    "slug": "srm-pay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/profile.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/profile.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/profile.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification-sound.wav",
            "./assets/sounds/notification-sound-other.wav"
          ]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/profile.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/sounds/notification-sound.wav",
            "./assets/sounds/notification-sound-other.wav"
          ]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/profile.png",
        "backgroundColor": "#ffff"
      },
      "package": "com.sonny067.srmpay"
    },
    "web": {
      "favicon": "./assets/images/profile.png"
    }
  }
}
